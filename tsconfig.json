{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@momentum/coinbase-client": [
        "libs/coinbase-client/src"
      ],
      "@momentum/coinbase-client/*": [
        "libs/coinbase-client/src/*"
      ],
      "@momentum/alpaca-client": [
        "libs/alpaca-client/src"
      ],
      "@momentum/alpaca-client/*": [
        "libs/alpaca-client/src/*"
      ],
      "@momentum/events": [
        "libs/events/src"
      ],
      "@momentum/events/*": [
        "libs/events/src/*"
      ],
      "@momentum/alpaca": [
        "libs/alpaca/src"
      ],
      "@momentum/alpaca/*": [
        "libs/alpaca/src/*"
      ],
      "@momentum/clock": [
        "libs/clock/src"
      ],
      "@momentum/clock/*": [
        "libs/clock/src/*"
      ]
    }
  }
}